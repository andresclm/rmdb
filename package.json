{
  "name": "rmdb",
  "version": "0.1.0",
  "keywords": [],
  "author": "Andres Lemus",
  "license": "ISC",
  "scripts": {
    "prestart": "tailwind build ./src/styles/index.scss -o ./src/styles/bundle.scss",
    "start": "webpack-dev-server --env.mode development",
    "start:dashboard": "npm run prestart && webpack-dashboard -c red -- webpack-dev-server --env.mode development --env.presets dashboard",
    "lint": "tsc && prettier -c . && eslint . --ext .js,.jsx,.ts,.tsx -f codeframe",
    "build:dev": "npm run prestart && webpack --env.mode development",
    "build:prod": "export NODE_ENV=production && npm run prestart && webpack --env.mode production",
    "build:prod:compress": "npm run build:prod -- --env.presets compress",
    "build:prod:analyze": "npm run build:prod -- --env.presets analyze"
  },
  "devDependencies": {
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "6.0.4",
    "css-loader": "5.0.0",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "1.2.0",
    "mini-svg-data-uri": "1.2.3",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "prettier": "2.1.2",
    "sass": "1.27.0",
    "sass-loader": "10.0.4",
    "style-loader": "2.0.0",
    "tailwindcss": "1.9.6",
    "terser-webpack-plugin": "5.0.1",
    "ts-loader": "8.0.7",
    "typescript": "4.0.5",
    "url-loader": "4.1.1",
    "webpack": "5.2.0",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dashboard": "3.2.1",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.2.0"
  },
  "dependencies": {
    "node-vibrant": "3.1.5",
    "react": "17.0.1",
    "react-circular-progressbar": "2.0.3",
    "react-dom": "17.0.1",
    "react-query": "2.25.2",
    "react-router-dom": "5.2.0",
    "swiper": "6.3.4"
  }
}
